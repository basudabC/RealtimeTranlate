{
    "version": 2,
    "builds": [
        {
            "src": "api/translate.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/api/translate",
            "dest": "api/translate.py"
        },
        {
            "src": "/static/(.*)",
            "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/translate.py"
        }
    ]
}